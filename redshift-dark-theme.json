  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#b8bb26",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#87ab63"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language.boolean"],
      "settings": {
        "foreground": "#cc580c"
      }
    },
    {
      "scope": ["constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#cc580c"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#b259d0"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#d0d5da"
      }
    },
    {
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#b259d0"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#3a8ac6"
      }
    },
    {
      "scope": ["entity.name.class", "entity.name.type", "support.class"],
      "settings": {
        "foreground": "#cc9903"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#cc4848"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#cc9903"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#d0d5da"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#cc580c"
      }
    },
    {
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#cc4848",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["support.type", "support.variable"],
      "settings": {
        "foreground": "#00918a"
      }
    },
    {
      "scope": ["meta.function-call"],
      "settings": {
        "foreground": "#3a8ac6"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#bcc3cb"
      }
    },
    {
      "scope": ["punctuation.definition.string"],
      "settings": {
        "foreground": "#87ab63"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#d22e8b"
      }
    },
    {
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#00918a"
      }
    },
    {
      "scope": ["entity.name.namespace", "entity.name.module"],
      "settings": {
        "foreground": "#cc9903"
      }
    },
    {
      "scope": ["meta.decorator", "punctuation.decorator"],
      "settings": {
        "foreground": "#906ae1"
      }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#87ab63"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#4183c4"
      }
    },
    {
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#4183c4"
      }
    },
    {
      "scope": ["entity.name.type.interface"],
      "settings": {
        "foreground": "#00918a"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#4183c4",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#87ab63"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#788797",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#cc9903"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#87ab63"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#cc4848"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#cc9903"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#cc4848",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["source.json support.type.property-name"],
      "settings": {
        "foreground": "#4183c4"
      }
    },
    {
      "scope": ["source.css entity.other.attribute-name"],
      "settings": {
        "foreground": "#cc9903"
      }
    },
    {
      "scope": ["source.css support.type.property-name"],
      "settings": {
        "foreground": "#4183c4"
      }
    },
    {
      "scope": ["source.css constant.other.color"],
      "settings": {
        "foreground": "#cc580c"
      }
    },
    {
      "scope": ["source.css support.constant"],
      "settings": {
        "foreground": "#00918a"
      }
    },
    {
      "scope": [
        "entity.name.function.macro",
        "entity.name.function.macro.rust",
        "support.function.macro.rust",
        "meta.macro"
      ],
      "settings": {
        "foreground": "#8ec07c"
      }
    }
  ]
}
